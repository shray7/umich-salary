# Azure two-region deployment (West US + East US)
# Copy to deploy-azure.env and set values. Do not commit deploy-azure.env.
#
# Required:
# export RESOURCE_GROUP=rg-umich-salary
# export ACR_NAME=umichsalaryacr
# export POSTGRES_SERVER_NAME=umich-salary-pg
# export POSTGRES_ADMIN_USER=pgadmin
# export POSTGRES_ADMIN_PASSWORD=<strong-password>
#
# Optional (defaults shown):
# export LOCATION_EAST=eastus
# export LOCATION_WEST=westus
# export POSTGRES_SKU=Standard_B1ms
# export AFD_PROFILE_NAME=umich-salary-fd
# export AFD_ENDPOINT_NAME=api
# export USE_READ_REPLICA=0

RESOURCE_GROUP=rg-umich-salary
LOCATION_EAST=eastus
LOCATION_WEST=westus
ACR_NAME=umichsalaryacr
POSTGRES_SERVER_NAME=umich-salary-pg
POSTGRES_ADMIN_USER=pgadmin
POSTGRES_ADMIN_PASSWORD=
POSTGRES_SKU=Standard_B1ms
AFD_PROFILE_NAME=umich-salary-fd
AFD_ENDPOINT_NAME=api
USE_READ_REPLICA=0
